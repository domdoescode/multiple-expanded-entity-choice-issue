<form action="" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}
    {{ form_rest(form) }}
    <input type="submit" />
</form>

<ul>
{% for category in categories %}
  <li>
    {{ category.name }}
    {% if category.products|length > 0 %}
    <ul>
      {% for product in category.products %}
        <li>{{ product.name }}</li>
      {% endfor %}
    </ul>
    {% endif %}
  </li>
{% endfor %}
</ul>